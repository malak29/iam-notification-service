SPRING_PROFILES_ACTIVE=prod

# Database Configuration
SPRING_R2DBC_URL=r2dbc:postgresql://prod-db:5432/iam_db
SPRING_R2DBC_USERNAME=${DB_USERNAME}
SPRING_R2DBC_PASSWORD=${DB_PASSWORD}

# Redis Configuration
SPRING_DATA_REDIS_HOST=${REDIS_HOST}
SPRING_DATA_REDIS_PORT=6379
SPRING_DATA_REDIS_PASSWORD=${REDIS_PASSWORD}

# Email Configuration (AWS SES for production)
EMAIL_PROVIDER=aws-ses
EMAIL_FROM=noreply@yourdomain.com
EMAIL_FROM_NAME=Your Company Name

# SMS Configuration (AWS SNS for production)
SMS_PROVIDER=aws-sns
SMS_FROM_NUMBER=+1234567890

# Template Configuration (AWS S3 for production)
TEMPLATE_STORAGE=aws-s3

# AWS Configuration (Production)
AWS_ENABLED=true
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=${AWS_ACCESS_KEY_ID}
AWS_SECRET_ACCESS_KEY=${AWS_SECRET_ACCESS_KEY}
AWS_S3_BUCKET=your-notification-templates-bucket

# JWT Configuration
JWT_SECRET=${JWT_SECRET}

# Queue Configuration
QUEUE_ENABLED=true
QUEUE_PROVIDER=redis

# Real-time Configuration
REALTIME_ENABLED=true